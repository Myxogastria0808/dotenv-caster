{
  "name": "dotenv-caster",
  "version": "2.0.6",
  "description": "A tiny dotenv caster.",
  "keywords": [
    "dotenv",
    ".env",
    "typescript",
    "primitive type"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npx tsc",
    "pub": "npm publish --access=public",
    "typedoc:github": "npx typedoc",
    "typedoc:local": "npx typedoc --out ./src/docs/",
    "install:typedoc": "npm i typedoc@0.25.13"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Myxogastria0808/dotenv-caster.git"
  },
  "author": "Yuki Osada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Myxogastria0808/dotenv-caster/issues"
  },
  "homepage": "https://github.com/Myxogastria0808/dotenv-caster#readme",
  "dependencies": {
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "typedoc": "^0.25.13"
  }
}
