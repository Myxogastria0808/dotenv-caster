{
  "name": "dotenv-caster",
  "version": "2.1.18",
  "description": "This package is a tiny package that converts `string | undefined` to certain `primitive types`.",
  "keywords": [
    "dotenv",
    ".env",
    "typescript",
    "primitive type"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "require": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "COPYING"
  ],
  "scripts": {
    "pub": "npm publish --access=public",
    "unpub": "npm unpublish  dotenv-caster@${npm_config_v}",
    "build": "tsup",
    "typedoc": "npx typedoc",
    "test": "vitest run --typecheck",
    "coverage": "vitest run --coverage --typecheck",
    "ui": "vitest run --ui --typecheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Myxogastria0808/dotenv-caster.git"
  },
  "author": "Yuki Osada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Myxogastria0808/dotenv-caster/issues"
  },
  "homepage": "https://myxogastria0808.github.io/dotenv-caster/",
  "packageManager": "pnpm@10.18.0",
  "peerDependencies": {
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.18.8",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dotenv": "^16.6.1",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.13",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
